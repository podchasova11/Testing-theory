SELECT * FROM teacher LIMIT 5;

SELECT id AS 'ИДЕНТИФИКАТОР', surname AS 'Фамилия' FROM teacher;

SELECT * FROM teacher ORDER BY surname;
SELECT * FROM teacher ORDER BY id
